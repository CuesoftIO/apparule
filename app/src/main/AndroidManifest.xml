<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.cuesoft.apparule">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".views.walkthrough.WalkThroughActivity">

        </activity>
        <activity android:name=".views.LandingActivity" />
        <activity android:name=".views.customer.CustomerActivity" />
        <activity
            android:name=".views.MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.designer.DesignerActivity" />
        <activity android:name=".views.BlankActivity" />
        <activity android:name=".views.SignInActivity" />
        <activity android:name=".views.customer.CustomerSignUpActivity" />
        <activity android:name=".views.designer.DesignerSignUpActivity" />
        <activity android:name=".views.DiscoveryActivity" />
        <activity android:name=".views.BaseActivity" />
        <activity android:name=".views.NotificationsActivity" />
        <activity android:name=".views.ProfileActivity" />
        <activity android:name=".views.PostActivity" />
        <activity android:name=".views.WalkThroughActivity">

        </activity>
    </application>

</manifest>